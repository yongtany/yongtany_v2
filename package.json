{
  "name": "mern-bolierplate",
  "version": "1.0.0",
  "description": "MERN stack bolierplate project",
  "main": "index.js",
  "engines": {
    "node": "11.10.1",
    "npm": "6.7.0",
    "yarn": "1.16.0"
  },
  "scripts": {
    "start:build": "tsc -w",
    "start:run": "nodemon dist/index.js",
    "dev": "concurrently npm:start:*",
    "build": "tsc",
    "postinstall": "npm run build",
    "start": "node ./dist/index.js",
    "client": "NODE_PATH=./client npm run start --prefix client",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client"
  },
  "keywords": [],
  "author": "yongtany",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^5.1.0",
    "nodemon": "^2.0.2"
  },
  "dependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/body-parser": "^1.17.1",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.2",
    "@types/joi": "^14.3.4",
    "@types/jsonwebtoken": "^8.3.5",
    "@types/moment": "^2.13.0",
    "@types/mongoose": "^5.5.38",
    "@types/morgan": "^1.7.37",
    "@types/multer": "^1.4.0",
    "@types/uuid": "^3.4.6",
    "bcrypt": "^3.0.7",
    "body-parser": "^1.19.0",
    "cloudinary": "^1.19.0",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-jwt": "^5.3.1",
    "express-validation": "^1.0.3",
    "express-validator": "^5.3.1",
    "http-status": "^1.4.2",
    "joi": "^14.3.1",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.24.0",
    "mongoose": "^5.8.4",
    "morgan": "^1.9.1",
    "multer": "^1.4.2",
    "ts-node": "^8.6.2",
    "typesafe-actions": "^5.1.0",
    "typescript": "^3.7.5",
    "uuid": "^3.3.2"
  }
}
